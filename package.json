{
  "name": "tanstack-wonderland",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,md}\"",
    "security:audit": "npm audit --audit-level=moderate",
    "security:check": "snyk test || true",
    "analyze": "vite-bundle-visualizer",
    "lighthouse": "lhci autorun",
    "prepare": "husky install || true",
    "pre-commit": "lint-staged",
    "docs:generate": "typedoc --out docs src",
    "validate": "npm run type-check && npm run lint && npm run test && npm run build"
  },
  "dependencies": {
    "@tanstack/react-form": "^0.35.0",
    "@tanstack/react-query": "^5.60.5",
    "@tanstack/react-query-devtools": "^5.60.5",
    "@tanstack/react-router": "^1.49.0",
    "@tanstack/react-table": "^8.20.5",
    "@tanstack/react-virtual": "^3.1.2",
    "@tanstack/zod-form-adapter": "^0.42.1",
    "@vitejs/plugin-react": "^4.3.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "vite": "^6.4.0",
    "vite-tsconfig-paths": "^5.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.48.0",
    "@tanstack/router-plugin": "^1.146.2",
    "@axe-core/react": "^4.9.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "axe-core": "^4.10.1",
    "@types/node": "^25.0.3",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "eslint-plugin-testing-library": "^7.1.2",
    "husky": "^9.1.7",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.11",
    "msw": "^2.6.4",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.14",
    "typedoc": "^0.28.15",
    "typescript": "^5.7.3",
    "vite-bundle-visualizer": "^1.2.1",
    "vitest": "^2.1.8",
    "i18next": "^24.1.0",
    "react-i18next": "^15.2.0",
    "i18next-browser-languagedetector": "^8.1.1"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,md,json}": [
      "prettier --write"
    ]
  }
}
